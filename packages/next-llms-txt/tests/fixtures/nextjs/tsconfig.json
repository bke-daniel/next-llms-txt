{
  "extends": "../../../../../tsconfig.base.json",
  "compilerOptions": {
    "incremental": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "baseUrl": ".",
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/fixtures/*": [
        "../*",
        "!../nextjs/*"
      ],
      "@/build-version": [
        "../../../dist/index.mjs"
      ],
      "@/code-version": [
        "../../../src/index.ts"
      ]
    },
    "allowJs": true,
    "noEmit": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "types": [
    "cypress"
  ],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts",
    "**/*.mts",
    "./cypress.d.ts",
    "cypress/*/*.ts"
  ],
  "exclude": [
    "cypress",
    "node_modules"
  ]
}
